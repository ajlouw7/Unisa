@article{subset,
abstract = {This paper describes the development of a SNOMED CT subset derived from clinical notes. A corpus of 44 million words of patient progress notes was drawn from the clinical information system of the Intensive Care Service (ICS) at the Royal Prince Alfred Hospital, Sydney, Australia. This corpus was processed by a variety of natural language processing procedures including the computation of all SNOMED CT candidate codes. There are about 13 million concept instances comprising about 30,000 unique concept types detected in the corpus. These instances have been processed by a tool which computes the closure of the minimal sub-tree of concept types in the SNOMED hierarchy thus inferring the complete subset of SNOMED CT that would be necessary for an intensive care unit. A subset of about 2700 concepts gives a coverage of 96{\%} of the corpus and the transitive closure uses less than 1{\%} of SNOMED concepts and relationships. Use of this subset will enable clinical information systems to efficiently deliver SNOMED CT terminology to the presentation interface.},
author = {Patrick, Jon and Wang, Yefeng and Budd, Peter and Rector, Alan and Brandt, Sebastian and Rogers, Jeremny and Herkes, Robert and Ryan, Angela and Vazirnezhad, Bahram},
file = {:C$\backslash$:/Users/Riaan/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Patrick et al. - 2008 - Developing SNOMED CT Subsets from Clinical Notes for Intensive Care Service.pdf:pdf},
issn = {1174-3379},
journal = {Heal. Care Informatics Rev. Online},
mendeley-groups = {Ontology Engineering/Found/Assignment 3,Ontology Engineering/Found/Assignment 3/Question 1/Maintenance},
number = {3},
pages = {25--30},
title = {{Developing SNOMED CT subsets from clinical notes for intensive care service}},
url = {https://cdn.ymaws.com/www.hinz.org.nz/resource/collection/ 0F09C2E4-7A05-49FB-8324-709F1AB2AA2F/F38{\_}Patrick.pdf},
volume = {12},
year = {2008}
}

